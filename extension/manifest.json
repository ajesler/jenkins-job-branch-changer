{
  "manifest_version": 2,

  "name": "Jenkins Job Git Branch Changer",
  "description": "Changes the git branch used by a list of Jenkins jobs.",
  "version": "0.1",
// "homepage_url": "https://github.com/..."

  "icons": { "48": "images/icon48.png" },
  "browser_action": {
    "default_icon": "images/icon48.png",
    "default_popup": "popup.html",
    "default_title": "Jenkins Branch Changer"
  },
  "permissions": [
    "storage",
    "https://*/",
    "http://*/"
  ],
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  }
}