{
  "manifest_version": 2,

  "name": "jenkins branch changer",
  "description": "Changes the git branch a jenkins job uses.",
  "version": "0.1",
  "icons": { "48": "images/icon48.png" },
  "browser_action": {
    "default_icon": "images/icon48.png",
    "default_popup": "popup.html",
    "default_title": "Jenkins Branch Changer"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  }
}